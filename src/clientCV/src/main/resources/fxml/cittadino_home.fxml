   <Button fx:id="button_idvaccinazione" alignment="CENTER" layoutX="328.0" layoutY="266.0" mnemonicParsing="false" onAction="#RecuperaIDVaccinazione" prefHeight="82.0" prefWidth="216.0" text="Recupera ID Vaccinazione" />
